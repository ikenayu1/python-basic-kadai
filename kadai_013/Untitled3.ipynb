        "def calculate_price_with_tax(price, tax_rate):\n",
        "    tax_amount = price * (tax_rate / 100)\n",
        "    total_price = price + tax_amount\n",
        "    return total_price\n",
        "\n",
        "price = 110\n",
        "tax_rate = 10\n",
        "\n",
        "total_price = calculate_price_with_tax(price, tax_rate)\n",
        "print(f\"消費税を含む総額は:{total_price}円です\")"
